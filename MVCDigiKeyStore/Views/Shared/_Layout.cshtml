<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="http://ajax.microsoft.com/ajax/jQuery/jquery-1.4.4.min.js" type="text/javascript"></script>
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet">
    @RenderSection("featured", required: false)
</head>

<body class="featured">
    <div id="header">
        <h1><a href="/">DIGI KEY STORE</a></h1>   <img src="/Content/Images/DigiKey.jpg" alt="" style="width:auto;height:auto;"> <img src="/Content/Images/info-banner.JPG" alt="" style="width:auto;height:auto;">
        <ul id="navlist">            
            <li class="first"><a href="@Url.Content("~")" id="current">Home</a></li>
            <li><a href="@Url.Content("~/Store/")">Store</a></li>
            <li>@{Html.RenderAction("CartSummary", "ShoppingCart");}</li>
            <li><a href="@Url.Content("~/StoreManager/")">Admin</a></li>
        </ul>
    </div>

     
    @{Html.RenderAction("TypeMenu", "Store");}
    <div id="main">        
        @RenderBody()         
    </div>


    <div id="footer">
        built with <a href="http://asp.net/mvc">ASP.NET MVC 3</a>
    </div>
    <img src="/Content/Images/Banner.png" alt="" style="width:auto;height:auto;">
</body>  
</html>
