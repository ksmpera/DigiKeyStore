@model MVCDigiKeyStore.Models.Type
 @{ ViewBag.Title = "Browse Products"; }
 <div class="type"> 
     <h3><em>@Model.Name</em>Products</h3>

      <ul id="product-list"> 
          @foreach (var product in Model.Products)
          { <li> <a href="@Url.Action("Details", new { id = product.ProductId })"> 
              <img alt="@product.ProductId" src="@product.ProductUrl" /> 
              <span>@product.ProductName</span> 

                 </a> 

            </li> } 

      </ul>

 </div>